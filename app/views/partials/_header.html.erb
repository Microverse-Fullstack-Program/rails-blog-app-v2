<% if user_signed_in? %>
  <header>
    <div class="header">
      <%=link_to root_path do %>
        <%= image_tag "home_icon.png", :alt => "Home" %>
      <% end %>

      <h1> Blog App </h1>

      <div>
        <%= link_to :back do %>
          <%= image_tag "back_icon.png", :alt => "Back" %>
        <% end %>
        <%= link_to destroy_user_session_path, data: { "turbo-method": :delete } do %> 
          <%= image_tag "logout_icon.png", :alt => "Logout" %>
        <% end %>
      </div

    </div>
  </header>
<% end %>
